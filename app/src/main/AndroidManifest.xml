<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.kincode.maths">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/math"
        android:label="@string/app_name"
        android:roundIcon="@drawable/math"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoogleSignIn_KT">
        <activity
            android:name=".acc_activity"
            android:exported="false"
            android:label="@string/title_activity_acc"
            android:theme="@style/Theme.GoogleSignIn_KT.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity2"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>